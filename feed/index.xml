---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <id>http://jcfausto.github.io/blog</id>
  <title>Blog - Julio Cesar Fausto</title>
  <icon>http://jcfausto.github.io/favicon.ico</icon>
  <link href="http://jcfausto.github.io/feed/index.xml" rel="self" />
  <link href="http://jcfausto.github.io/blog" />
  {% for post in site.posts limit:10 %}
  {% if post.layout == 'post' %}
  <entry>
    <id>http://jcfausto.github.io{{ post.id }}</id>
    <title>{{ post.title | strip_html }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <link href="http://jcfausto.github.io{{ post.url }}" />
    <author>
      <name>Julio Cesar Fausto</name>
      <uri>http://jcfausto.github.io/</uri>
    </author>
    <content type="html">
      {{ post.content | replace: "src='/assets/", "src='http://jcfausto.hithub.io/assets/" | xml_escape }}
    </content>
  </entry>
  {% endif %}
  {% endfor %}
</feed>